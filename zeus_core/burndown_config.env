# Burndown Chart Configuration
# Set these environment variables before running the burndown system

# Output directory for charts
export BURNDOWN_OUT=charts

# S3 bucket for chart uploads
export BURNDOWN_BUCKET=zeus-charts

# AWS Configuration (set these for S3 uploads)
# export AWS_ACCESS_KEY_ID=your_access_key
# export AWS_SECRET_ACCESS_KEY=your_secret_key
# export AWS_DEFAULT_REGION=us-east-1

# Slack/Teams webhook (set for notifications)
# export SLACK_WEBHOOK_URL=https://hooks.slack.com/services/...
